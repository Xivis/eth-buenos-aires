{
  "env": {
    "browser": true,
    "es6": true
  },
  "extends": ["airbnb", "prettier", "prettier/react"],
  "plugins": ["prettier", "jsx-a11y"],
  "rules": {
    "no-console": "off",
    "consistent-return": "off",
    "import/no-extraneous-dependencies": "off",
    "prettier/prettier": "error",
    "react/jsx-filename-extension": [
      1,
      {
        "extensions": [".js", ".jsx"]
      }
    ]
  },
  "settings": {
    "react": {
      "version": "detect"
    }
  }
}
